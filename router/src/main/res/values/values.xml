<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RouteView">
        <attr name="routeUri" format="string" />
        <attr name="absentVisibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
        <attr name="onClickUri" format="string" />
    </declare-styleable>

    <declare-styleable name="RouteLayout">
        <attr name="conditionUri" format="string" />
        <attr name="slotViewId" format="reference" />
    </declare-styleable>
</resources>
